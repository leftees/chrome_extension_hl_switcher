{
    "env": {
        "browser": true,
        "es6": true,
        "jasmine": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "chrome": false,
        "module": false,
        "process": false
    },
    "rules": {
        "indent": [
            "warning",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}
